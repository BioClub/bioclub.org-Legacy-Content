					mapTypeId:google.maps.MapTypeId.ROADMAP,
					styles: lelStyle,
					scrollwheel: false
				});
				createMarker ({
					position : new google.maps.LatLng(35.65606110, 139.69549320),
					map : mapCanvas
				});
				function createMarker (opts) {
					var marker = new google.maps.Marker(opts);
					return marker;
				}
			}
			initializeGmap();
		</script>
	</body>
</html>

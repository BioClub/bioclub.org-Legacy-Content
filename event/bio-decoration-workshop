					"stylers": [
					{ "visibility": "off" }
					]
				  },{
					"elementType": "labels.text.fill",
					"stylers": [
					{ "gamma": 1.54 }
					]
				  },{
					"featureType": "water",
					"elementType": "labels",
					"stylers": [
					{ "visibility": "off" }
					]
				  },
				  {
					"featureType": "water",
					"stylers": [
					{ "lightness": 50 },
					{ "hue": "#0088ff" }
					]
				  },
					{
						"featureType": "all",
						"stylers": [
							{ "saturation": -100 },
							{"lightness": 10}
						]
					}
				]
				var mapDiv = document.getElementById("gmap");
				var mapCanvas = new google.maps.Map(mapDiv, {
					center:new google.maps.LatLng(35.65606110, 139.69549320),
					zoom:15,
					mapTypeId:google.maps.MapTypeId.ROADMAP,
					styles: lelStyle,
					scrollwheel: false
				});
				createMarker ({
					position : new google.maps.LatLng(35.65606110, 139.69549320),
					map : mapCanvas
				});
				function createMarker (opts) {
					var marker = new google.maps.Marker(opts);
					return marker;
				}
			}
			initializeGmap();

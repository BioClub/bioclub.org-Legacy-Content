				createMarker ({
					position : new google.maps.LatLng(35.65606300, 139.69549100),
					map : mapCanvas
				});
				function createMarker (opts) {
					var marker = new google.maps.Marker(opts);
					return marker;
				}
			}
			initializeGmap();
		</script>
	</body>
</html>
